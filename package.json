{
  "name": "reactjs-d3-universal",
  "version": "0.0.8",
  "description": "For all of your progressive data visualization needs",
  "keywords": [
    "reactjs",
    "d3",
    "PWA",
    "progressive web app",
    "isomorphic",
    "universal",
    "charts",
    "tables"
  ],
  "homepage": "https://github.com/noahehall/reactjs-d3-universal",
  "bugs": {
    "url": "https://github.com/noahehall/reactjs-d3-universal/issues",
    "email": "noahedwardhall@gmail.com"
  },
  "license": "MPL-2.0",
  "author": "noah edward hall <noahedwardhall@gmail.com> (https://twitter.com/noahedwardhall)",
  "repository": {
    "type": "git",
    "url": "https://github.com/noahehall/reactjs-d3-universal.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "open example/index.html",
    "example": "./node_modules/.bin/browserify example/app.js -o example/runapp.js -t [ babelify --presets [ latest stage-0 react ] ] -t [ uglifyify --compress --mangle ] && npm run start",
    "build": "./node_modules/.bin/babel src --out-dir dist"
  },
  "engines": {
    "node": ">=6",
    "npm": " >=4"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "d3": "^4.4.0",
    "d3-scale-chromatic": "^1.1.0",
    "node-globals": "github:noahehall/node-globals",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7",
    "babili": "0.0.9",
    "eslint-config-ct-fletcher": "^1.0.4",
    "uglifyify": "^3.0.4"
  }
}
